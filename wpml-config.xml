			<key name="easy-digital-downloads">
				<key name="label" />
			</key>
			<key name="contact-form-7">
				<key name="label" />
			</key>
			<key name="wp-registration-form">
				<key name="label" />
			</key>
			<key name="wp-comment-form">
				<key name="label" />
			</key>
			<key name="ninja-forms">
				<key name="label" />
			</key>
			<key name="events-manager">
				<key name="label" />
			</key>
			<key name="buddypress">
				<key name="label" />
			</key>
		</key>
	</admin-texts>
</wpml-config>


